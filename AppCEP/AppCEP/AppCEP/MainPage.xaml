<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="AppCEP.MainPage">

    <StackLayout BackgroundColor="#FFBF00" Padding="30" Spacing="30">
        <Image Source="LogoCorreios.png" x:Name="Logo"/>
        <Entry Placeholder="Digite o cep" PlaceholderColor="#FFBF00" BackgroundColor="White" x:Name="Cep"/>
        <Button Text="BUSCAR CEP" TextColor="White" BackgroundColor="#00416B" x:Name="Botao"/>
        <Label x:Name="Resultado"/>

    </StackLayout>


</ContentPage>